{
  "name": "TimeTracker",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --env.prod -p",
    "postbuild": "cpy src/favicon.ico dist",
    "prebuild:dev": "rimraf dist",
    "build:dev": "webpack --env.dev",
    "postbuild:dev": "cpy src/favicon.ico dist",
    "start": "npm run dev",
    "dev": "webpack-dev-server --env.dev",
    "validate": "npm-run-all --parallel lint build test",
    "setup": "npm install && npm run validate",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test jest",
    "coverage": "NODE_ENV=test jest --coverage"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.4.2",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "offline-plugin": "^4.8.4",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-range": "^3.0.3",
    "normalize.css": "^7.0.0",
    "normalizr": "^3.2.3",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-google-login": "^2.11.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "modulePaths": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "config": "<rootDir>/.env.js",
      "components": "<rootDir>/src/components/",
      "services": "<rootDir>/src/services",
      "state": "<rootDir>/src/state",
      "utils": "<rootDir>/src/utils",
      "views": "<rootDir>/src/views",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy"
    }
  }
}
