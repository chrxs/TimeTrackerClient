{
  "name": "TimeTracker",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --env.prod -p",
    "postbuild": "cpy src/favicon.ico dist",
    "prebuild:dev": "rimraf dist",
    "build:dev": "webpack --env.dev",
    "postbuild:dev": "cpy src/favicon.ico dist",
    "start": "npm run dev",
    "dev": "webpack-dev-server --env.dev",
    "validate": "npm-run-all --parallel lint build test",
    "setup": "npm install && npm run validate",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test jest",
    "coverage": "NODE_ENV=test jest --coverage"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jest": "^19.0.2",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "offline-plugin": "^4.6.2",
    "postcss-loader": "^1.3.3",
    "pre-commit": "^1.2.2",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "camelcase-keys": "^4.1.0",
    "lodash": "^4.17.4",
    "normalize.css": "^6.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-google-login": "^2.8.6",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0",
    "snakecase-keys": "^1.1.0",
    "whatwg-fetch": "^2.0.3"
  },
