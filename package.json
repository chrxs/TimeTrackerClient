    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-promise": "^3.5.0",
